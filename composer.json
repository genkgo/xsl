{
    "name" : "genkgo/xsl",
    "description": "XSL 2.0 Transpiler in PHP",
    "require" : {
        "php" : "^7.4 || ~8.0.0 || ~8.1.0",
        "ext-json" : "*",
        "ext-libxml" : "*",
        "ext-xsl" : "*",
        "ext-dom" : "*",
        "ext-intl" : "*",
        "psr/simple-cache": "^1.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload" : {
        "psr-4" : {
            "Genkgo\\Xsl\\" : ["src"]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Genkgo\\Xsl\\" : ["test"]
        }
    }
}
