{
    "name" : "genkgo/xsl",
    "description": "XSL 2.0 Transpiler in PHP",
    "require" : {
        "php" : ">=7.3",
        "ext-json" : "*",
        "ext-libxml" : "*",
        "ext-xsl" : "*",
        "ext-dom" : "*",
        "ext-intl" : "*",
        "psr/simple-cache": "^1.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "^8.5.8",
        "phpstan/phpstan": "^0.11.20",
        "phpstan/phpstan-phpunit": "^0.11.2",
        "friendsofphp/php-cs-fixer": "^2.16.2"
    },
    "autoload" : {
        "psr-4" : {
            "Genkgo\\Xsl\\" : ["src"]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Genkgo\\Xsl\\" : ["test"]
        }
    }
}
